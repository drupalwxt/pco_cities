    ],
    "scripts": {
        "post-install-cmd": [
            "@composer drupal-scaffold"
        ],
        "post-update-cmd": [
            "@composer package"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "package": "Drupal\\pco_cities\\Composer\\Package::execute",
        "release-version": "Drupal\\pco_cities\\Composer\\ReleaseVersion::execute",
        "verify-patched-constraints": "Drupal\\pco_cities\\Composer\\PatchedConstraint::execute"
    },
    "extra": {
        "installer-types": [
          "bower-asset",
          "npm-asset"
        ],
        "installer-paths": {
            "html/core": [
                "type:drupal-core"
            ],
            "html/libraries/{$name}" : [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "html/modules/custom/{$name}": [
                "drupal/wxt_library"
            ],
            "html/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "html/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "html/themes/custom/{$name}": [
                "drupal/wxt_bootstrap"
            ],
            "html/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true,
        "patches": { }
    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.0",
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "drupalwxt/wxt": "2.0.1",
        "harvesthq/chosen": "1.6.1"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\pco_cities\\": "src"
        }
    }
}
